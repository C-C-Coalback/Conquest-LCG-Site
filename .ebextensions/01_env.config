option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: <PROJECT_DIR/.../>wsgi.py
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: <PROJECT_DIR...CONFIG.FILE>
    PYTHONPATH: /opt/python/current/app/<PROJECT_DIR>:$PYTHONPATH
  aws:elbv2:listener:8080:
    ListenerEnabled: 'true'
    Protocol: HTTP

  aws:elbv2:listener:6379:
    ListenerEnabled: 'true'
    Protocol: HTTP